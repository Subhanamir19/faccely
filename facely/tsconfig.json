{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["*"],
      "@components/*": ["components/*"],
      "@lib/*": ["lib/*"],
      "@store/*": ["store/*"],
      "@stores/*": ["store/*"],
      "@/components/*": ["components/*"],
      "@/lib/*": ["lib/*"],
      "@/store/*": ["store/*"],
      "@/stores/*": ["store/*"]
    }
  }
}
