{
  "expo": {
    "name": "Sigma Max",
    "slug": "sigmamax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",

    "newArchEnabled": false,

    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.sigmamax.app",

      
      "versionCode": 4,

      
      "minSdkVersion": 21,
      "targetSdkVersion": 34,

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },

      "edgeToEdgeEnabled": true
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      "expo-router"
    ],

    "scheme": "sigmamax",

    "assetBundlePatterns": [
      "assets/fonts/*",
      "assets/**/*"
    ],

    "extra": {
      "EXPO_PUBLIC_API_BASE_URL": "https://faccely-production.up.railway.app",

      "EXPO_PUBLIC_FIREBASE_API_KEY": "UNUSED",
      "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "UNUSED",
      "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "UNUSED",
      "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "UNUSED",
      "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "UNUSED",
      "EXPO_PUBLIC_FIREBASE_APP_ID": "UNUSED",
      "EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID": "UNUSED",

      "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "",

      "router": {},
      "eas": {
        "projectId": "897d04ae-b434-4398-8032-cb9cb1b1a918"
      }
    },

    "owner": "subhan_amir"
  }
}
